{
  "manifest_version": 3,
  "name": "Tonic",
  "version": "1.0.0",
  "description": "Take a breather",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'"
  }
}
